package graficos;

import java.awt.Color;

public class Cores {
	private int red;
	private int green;
	private int blue;
	private Color cor;
	
	/**
	 * Construtor, recebe o valor das intensidades
	 * @param red
	 * @param green
	 * @param blue
	 */
	public Cores(Color cor){
		this.cor = new Color(red, green, blue);
	}

	/**
	 * Retorna intensidade vermelha
	 * @return
	 */
	public int getCor() {
		return red;
	}
	
	/**
	 * Retorna intensidade verde
	 * @return
	 */
	public int getGreen() {
		return green;
	}
	/**
	 * Retorna intensidade azul
	 * @return
	 */
	public int getBlue() {
		return blue;
	}

	/**
	 * Retorna objeto do tipo Color
	 * @return
	 */
	public Color getCor() {
		return cor;
	}
	
	
}
